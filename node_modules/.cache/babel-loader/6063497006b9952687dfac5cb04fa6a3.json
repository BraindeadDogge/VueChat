       <ChatMessage  v-for=\"smth in text\" :key=\"smth.id\" :username=\"smth.author\" :datetime=\"smth.datetime\">{{ smth.text }}</ChatMessage>\n      </ChatWindow>\n    </Container>\n  </div>\n</template>\n\n<script>\nimport Container from './components/Container'\nimport ChatMessage from './components/ChatMessage'\nimport ChatWindow from './components/ChatWindow'\nimport axios from 'axios'\n\nexport default {\n  name: 'app',\n  components: {\n    Container,\n    ChatMessage,\n    ChatWindow\n  },\n  data() {\n    return {\n      text: []\n    }\n  },\n  methods: {\n    getMessages(){\n      setInterval(() => {\n        axios.get('http://188.225.47.187/api/chat/getmessages.php').then((response) => {\n        this.text = response.data\n        })\n      }, 1000)\n    },\n    sendMessage(obj){\n      if(obj.message !== \"\") {\n      obj.nickname, obj.message\n      axios.post('http://188.225.47.187/api/chat/sendmessage.php', obj)({\n        author: obj.nickname,\n        text: obj.message,\n      }).then((response) =>{\n        console.log(\"obj\", obj)\n        console.log('rrr', response)\n        this.getMessages()\n      })\n      }\n    },\n  },\n  mounted() {\n    this.getMessages()\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\nbody {\n  margin: 0;\n  background-color: #f9f9fa;\n}\n</style>\n"],"sourceRoot":"src"}]}